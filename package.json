  "description": "starter library for angular4 component modules",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "./build.sh",
    "clean": "rimraf build dist tmp",
    "commit": "git-cz",
    "prebuild": "npm run clean",
    "precommit": "npm test",
    "reset": "npm run clean & rimraf node_modules",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "serve": "lite-server -c=.bs-config.json",
    "start": "npm run serve",
    "test": "./test.sh"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chase2981/angular4-lib-starter.git"
  },
  "keywords": [
