{
  "name": "angular4-lib-starter",
  "version": "0.0.0-development",
  "description": "starter library for angular4 component modules",
  "main": "dist/index.js",
  "scripts": {
    "test:once": "npm run build:tsc && karma start karma.conf.js --single-run",
    "build:tsc": "tsc -p src/",
    "build-aot": "ngc -p tsconfig-aot.json && rollup -c rollup-config.js",
    "build": "./rollup.sh",
    "clean": "rimraf build dist aot coverage webpack",
    "commit": "git-cz",
    "prebuild": "rimraf dist",
    "reset": "npm run clean & rimraf node_modules",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "serve": "lite-server -c=bs-config.json",
    "serve:aot": "lite-server -c bs-config.aot.json",
    "start": "npm run serve",
    "test": "./test.sh"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chase2981/angular4-lib-starter.git"
  },
  "keywords": [
    "angular",
    "lib",
    "angular4",
    "starter",
    "component",
    "modules",
    "advanced"
  ],
  "author": "chase g. <cwgibb@aggiemail.usu.edu> (http://turbonemesis.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chase2981/angular4-lib-starter/issues"
  },
  "homepage": "https://github.com/chase2981/angular4-lib-starter#readme",
  "dependencies": {
    "@angular/common": "~4.2.0",
    "@angular/compiler": "~4.2.0",
    "@angular/compiler-cli": "~4.2.0",
    "@angular/core": "~4.2.0",
    "@angular/forms": "~4.2.0",
    "@angular/http": "~4.2.0",
    "@angular/platform-browser": "~4.2.0",
    "@angular/platform-browser-dynamic": "~4.2.0",
    "@angular/platform-server": "~4.2.0",
    "@angular/router": "~4.2.0",
    "@angular/tsc-wrapped": "~4.2.0",
    "angular-in-memory-web-api": "~0.3.2",
    "core-js": "^2.4.1",
    "rxjs": "^5.1.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "angular2-template-loader": "0.6.2",
    "awesome-typescript-loader": "3.1.3",
    "babel-cli": "6.11.4",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.13.2",
    "babel-preset-stage-2": "6.13.0",
    "babel-register": "6.11.6",
    "codelyzer": "~2.0.0",
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.0.0",
    "husky": "^0.13.3",
    "jasmine": "^2.5.2",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "json-loader": "0.5.4",
    "karma": "1.6.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-typescript": "3.0.4",
    "karma-webpack": "^1.8.0",
    "lite-server": "2.3.0",
    "minimatch": "^3.0.3",
    "node-static": "^0.7.9",
    "nyc": "^10.3.0",
    "raw-loader": "0.5.1",
    "rimraf": "^2.6.1",
    "rollup": "0.41.6",
    "rollup-plugin-commonjs": "8.0.2",
    "rollup-plugin-node-resolve": "3.0.0",
    "rollup-plugin-uglify": "1.0.2",
    "semantic-release": "^6.3.2",
    "systemjs": "0.19.39",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "typescript": "~2.3.2",
    "webpack": "2.4.1",
    "webpack-dev-server": "2.4.5",
    "yargs": "^7.1.0",

    "babel-preset-angular2": "^0.0.2",
    "canonical-path": "0.0.2",
    "concurrently": "^3.0.0",
    "http-server": "^0.9.0",
    "lodash": "^4.16.2",
    "phantomjs-prebuilt": "^2.1.7",
    "protractor": "~5.1.0",
    "source-map-explorer": "^1.3.2"
  }
}
